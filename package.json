{
  "name": "martrix",
  "description": "A maximalist, ultra-modern, opinionated blog starter for rich, interactive storytelling",
  "version": "0.0.1",
  "private": true,
  "author": "@elmarsto",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "npx next",
    "build": "npx next build",
    "start": "npx next start",
    "type-check": "npx tsc --noEmit",
    "lint": "npx eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "npx prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "npx husky install",
    "storybook": "storybook dev -s ./public -p 6006",
    "build-storybook": "storybook build -s ./public"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
